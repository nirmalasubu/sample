{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "args": [],
    "tasks": [
        {
            "taskName": "build",
            "args": [ 
                "${workspaceRoot}/OnDemandTools.API/project.json"
            ],
            "isBuildCommand": true,
            "showOutput": "silent",
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "restore",
            "args": [ 
                "${workspaceRoot}/OnDemandTools.API/project.json",
                "${workspaceRoot}/OnDemandTools.Common/project.json",
                "${workspaceRoot}/OnDemandTools.DAL/project.json",
                "${workspaceRoot}/OnDemandTools.Business/project.json",
                "${workspaceRoot}/OnDemandTools.API.Utilities/project.json"
            ],
            "isBuildCommand": true,
            "showOutput": "silent",
            "problemMatcher": "$msCompile"
        }
    ]
}